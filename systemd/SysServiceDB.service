[Unit] 
Description=Place file in /etc/systemd/system	Service to reboot the consumers on the DB so the queue doesnt get connections stuck in queue
After=network.target
StartLimitIntervalSec=0

[Service]
Type=simple
Restart=always
RestartSec=1
User=saj39
ExecStart=/usr/bin/env php /home/saj39/IT490-Sample/rpc_register_consumer.php
ExecStartPost=/usr/bin/env php /home/saj39/IT490-Sample/rpc_login_consumer.php

[Install]
WantedBy=multi-user.target